{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::phoenix-storage-dev/*",
                "arn:aws:s3:::phoenix-storage-dev",
                "arn:aws:s3:::phoenix-storage-prod-final/*",
                "arn:aws:s3:::phoenix-storage-prod-final"
            ]
        }
    ]
} 