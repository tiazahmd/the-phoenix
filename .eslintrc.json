{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "indent": ["error", 4],
    "max-len": ["error", { "code": 120 }],
    "comma-dangle": ["error", "never"]
  }
} 